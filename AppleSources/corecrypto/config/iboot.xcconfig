INSTALL_PATH = /usr/local/lib
HEADER_SEARCH_PATHS = $(SRCROOT)/cc $(SRCROOT)/ccmode $(SRCROOT)/cccmac $(SRCROOT)/ccrng $(SRCROOT)/cc_fips $(SRCROOT)/ccec25519 $(TOOLCHAIN_DIR)/usr/include $(inherited) $(SRCROOT)/acceleratecrypto/Header

OTHER_CFLAGS = $(inherited) -fstack-protector-strong
OTHER_CFLAGS = $(inherited) -Wno-error=#warnings
OTHER_CFLAGS = $(inherited) -Wundef
OTHER_CFLAGS = $(inherited) -Wundef-prefix=TARGET_
OTHER_CFLAGS = $(inherited) -fno-typed-memory-operations

GCC_PREPROCESSOR_DEFINITIONS = IBOOT $(inherited)
