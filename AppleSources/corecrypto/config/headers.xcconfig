ARCHS[sdk=macosx*] = $(ARCHS_STANDARD)
SDKROOT =
SKIP_INSTALL = YES
DEFINES_MODULE = YES
EXECUTABLE_EXTENSION = empty.a
MODULEMAP_PRIVATE_FILE = $(SRCROOT)/module.private.modulemap
PRODUCT_NAME = corecrypto
CLANG_ENABLE_MODULES = YES
CLANG_ENABLE_OBJC_WEAK = YES
CLANG_WARN_IMPLICIT_SIGN_CONVERSION = YES
CLANG_MODULES_ENABLE_VERIFIER_TOOL = YES
INSTALLHDRS_SCRIPT_PHASE = YES

PUBLIC_HEADERS_FOLDER_PATH = $(SDK_INSTALL_HEADERS_ROOT)/usr/local/include/corecrypto
PRIVATE_HEADERS_FOLDER_PATH = $(SDK_INSTALL_HEADERS_ROOT)/usr/local/include/corecrypto
